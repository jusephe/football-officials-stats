{% extends 'admin/base.html.twig' %}

{% block css %}
    <link rel="stylesheet" type="text/css" href="{{ asset('styles/admin/form.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('styles/admin/add_edit_nomination_lists.css') }}">
{% endblock %}

{% block title %}Rozhodčí PFS - Změna listin{% endblock %}

{% block heading %}Změna listin{% endblock %}

{% block main %}

    <h2 class="h4">{{ official.name }}</h2>

    <p>
        Zde je možné změnit již zadané listiny daného rozhodčího. (Listiny jsou řazeny vzestupně podle roku.)
    </p>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <input type="submit" class="btn btn-primary btn-save" value="Uložit"/>
    {{ form_end(form) }}

{% endblock %}
